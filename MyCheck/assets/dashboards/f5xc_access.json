                {
                  "formula": "a",
                  "limit": {
                    "count": 5,
                    "order": "desc"
                  }
                }
              ],
              "style": {
                "palette": "datadog16"
              },
              "response_format": "scalar",
              "queries": [
                {
                  "search": {
                    "query": "$Namespace $Domain $Country"
                  },
                  "data_source": "logs",
                  "compute": {
                    "aggregation": "count"
                  },
                  "name": "a",
                  "indexes": [
                    "*"
                  ],
                  "group_by": [
                    {
                      "facet": "@namespace",
                      "sort": {
                        "aggregation": "count",
                        "order": "desc"
